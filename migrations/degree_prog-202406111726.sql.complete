INSERT INTO "degree_prog" ("degree_prog_id", "inst_name_cn", "inst_name_en", "intro_cn", "inst_type", "country", "rank", "prog_name", "is_indep", "prog_type", "prog_url", "school", "host_inst", "awards_or_abet", "apply_url", "degree", "duration", "joint", "min_credit", "thesis", "prog_goal", "prog_outcome", "has_culm_act", "internship ", "has_minor", "course_url", "faculty", "stu_hb_url") VALUES (28, '康奈尔大学', 'Cornell University', '康奈尔大学（Cornell University），位于美国纽约州伊萨卡，是一所世界顶级私立研究型大学（另有两个校区位于纽约市和卡塔尔教育城），为美国大学协会的十四个创始院校之一，以及著名的常春藤盟校的八个成员之一。康奈尔大学由埃兹拉·康奈尔和安德鲁·迪克森·怀特于1865年建立，是常春藤八盟校中唯一创建于美国独立战争之后的新生力量，其办学理念影响了整个美国高等教育，办学规模为当时全美高校之最。康奈尔大学的立校之本是任何人都有获得教育的平等权利，是常春藤盟校中第一所实行性别平等的男女合校大学，在招生录取上最早实行不计贵族身份，不分信仰和种族，并以创建学科齐全、包罗万象的新型综合性大学为建校宗旨。康奈尔大学是一所采用公私合营的办学模式的大学，最初以农工学院为特色而起家，其酒店管理学院、工业与劳工关系学院为全美首创。康奈尔大学专业包括农业、兽医、工科、计算机科学、劳工关系、文理、经济、建筑、城市规划、教育、商科、传媒、以及酒店管理等。截止2018年，共有58位康奈尔大学的校友或教研人员曾荣获诺贝尔奖，在全球高校中列第十二位，居全美第十位。2019-20年度，位列2020QS世界大学排名世界第14，2019软科世界大学学术排名世界第13，2020莫斯科国际大学排名世界第18，2020年THE世界大学排名世界第19，2020U.S. News世界大学排名世界第23；2019福布斯美国大学排行榜第11，2020U.S. News美国最佳大学排名第17。2021年QS世界大学排名世界第18。', '私立', '美国', 16, 'Computer Science-Artificial Intelligence', 0, '学术型硕士', 'http://www.cs.cornell.edu/ms', 'Cornell Ann S. Bowers College of Computing and Information Science. ', 'Department of Computer Science', NULL, 'https://gradschool.cornell.edu/admissions/apply/apply-now/', 'Master of Science in computer science ', '1 Year to Less than 5 Years', '否', '34units', 'https://gradschool.cornell.edu/academic-progress/requirements/making-academic-progress/defending-your-thesis-or-dissertation/', 'for students who wish to deepen their knowledge of computer science through advanced coursework, research, writing, and teaching.', 'Demonstrate knowledge at the undergraduate level of four core areas in computer science

Systems, AI, PL, and theory
Students should be able to solve problems in these four core areas at the undergraduate level.
Demonstrate knowledge, understanding, applications, analysis, and evaluation of material at the graduate level in four out of five important core areas of computer science

The five different core areas are as follows: algorithms and theory of computation, artificial intelligence, systems, programming languages and methodology, scientific computing and applications
Demonstrate knowledge, understanding, applications, analysis, and evaluation of the three different value systems of the three computer science research styles that differ in how they evaluate and validate research results

The three different styles are: theory, systems, and applied', 0, NULL, 0, 'https://www.cs.cornell.edu/courseinfo/listofcscourses', 'https://gradschool.cornell.edu/academics/fields-of-study/field/computer-science/', NULL);
INSERT INTO "degree_prog" ("degree_prog_id", "inst_name_cn", "inst_name_en", "intro_cn", "inst_type", "country", "rank", "prog_name", "is_indep", "prog_type", "prog_url", "school", "host_inst", "awards_or_abet", "apply_url", "degree", "duration", "joint", "min_credit", "thesis", "prog_goal", "prog_outcome", "has_culm_act", "internship ", "has_minor", "course_url", "faculty", "stu_hb_url") VALUES (29, '康奈尔大学', 'Cornell University', '康奈尔大学（Cornell University），位于美国纽约州伊萨卡，是一所世界顶级私立研究型大学（另有两个校区位于纽约市和卡塔尔教育城），为美国大学协会的十四个创始院校之一，以及著名的常春藤盟校的八个成员之一。康奈尔大学由埃兹拉·康奈尔和安德鲁·迪克森·怀特于1865年建立，是常春藤八盟校中唯一创建于美国独立战争之后的新生力量，其办学理念影响了整个美国高等教育，办学规模为当时全美高校之最。康奈尔大学的立校之本是任何人都有获得教育的平等权利，是常春藤盟校中第一所实行性别平等的男女合校大学，在招生录取上最早实行不计贵族身份，不分信仰和种族，并以创建学科齐全、包罗万象的新型综合性大学为建校宗旨。康奈尔大学是一所采用公私合营的办学模式的大学，最初以农工学院为特色而起家，其酒店管理学院、工业与劳工关系学院为全美首创。康奈尔大学专业包括农业、兽医、工科、计算机科学、劳工关系、文理、经济、建筑、城市规划、教育、商科、传媒、以及酒店管理等。截止2018年，共有58位康奈尔大学的校友或教研人员曾荣获诺贝尔奖，在全球高校中列第十二位，居全美第十位。2019-20年度，位列2020QS世界大学排名世界第14，2019软科世界大学学术排名世界第13，2020莫斯科国际大学排名世界第18，2020年THE世界大学排名世界第19，2020U.S. News世界大学排名世界第23；2019福布斯美国大学排行榜第11，2020U.S. News美国最佳大学排名第17。2021年QS世界大学排名世界第18。', '私立', '美国', 16, 'Computer Science-Artificial Intelligence', 0, '专业型硕士', 'https://www.cs.cornell.edu/masters', 'Cornell Ann S. Bowers College of Computing and Information Science. ', 'Department of Computer Science', NULL, 'https://gradschool.cornell.edu/admissions/apply/', 'Master of Engineering in Computer Science', '1 Year to Less than 5 Years', '否', '30units', NULL, 'designed to enhance practical skills.', 'Mastery and Application of Core Disciplinary Knowledge
Problem Formulation, Organization, and Planning of the Solution Process
Collaborative Problem Solving and Issue Resolution
Communication of Knowledge, Ideas, and Decision Justification
Self-directed Learning and Professional Development', 1, NULL, 1, 'https://www.cs.cornell.edu/courseinfo/listofcscourses', 'https://gradschool.cornell.edu/academics/fields-of-study/field/computer-science/', NULL);

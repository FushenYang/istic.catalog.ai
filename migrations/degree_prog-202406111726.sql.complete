INSERT INTO "degree_prog" ("degree_prog_id", "inst_name_cn", "inst_name_en", "intro_cn", "inst_type", "country", "rank", "prog_name", "is_indep", "prog_type", "prog_url", "school", "host_inst", "awards_or_abet", "apply_url", "degree", "duration", "joint", "min_credit", "thesis", "prog_goal", "prog_outcome", "has_culm_act", "internship ", "has_minor", "course_url", "faculty", "stu_hb_url") VALUES (8, '加州大学伯克利分校', 'University of California，Berkeley', '加利福尼亚大学（University of California），简称加州大学，是位于美国加利福尼亚州的一个由10所公立大学组成的大学行政系统，是世界上最具影响力的公立大学系统，被誉为“公立高等教育的典范”。加州大学起源于1853年建立在奥克兰的加利福尼亚学院（College of California），1868年3月正式更名为“加州大学”。1873年，学校迁入新址，为了纪念18世纪最伟大的哲学家之一乔治·贝克莱，新的大学城被命名为“伯克利市”，此后逐渐在洛杉矶等地开设分校区。1952年起，“加州大学”作为一个行政系统逐渐与伯克利加州大学分离。与此同时，加州各地的分校区也逐渐升格为与伯克利平级的、独立的大学。如今，加州大学已发展成一个拥有10所公立大学并对世界发展影响深远的大学系统。这些校区包括加州大学伯克利分校（UC Berkeley）、加州大学洛杉矶分校（UCLA）、加州大学圣地亚哥分校（UCSD）、加州大学旧金山分校（UCSF）、加州大学圣塔芭芭拉分校（UCSB）、加州大学尔湾分校（UCI）、加州大学戴维斯分校（UCD）、加州大学圣克鲁兹分校（UCSC）、加州大学河滨分校（UCR）和加州大学美熹德分校（UCM）。（注：现今的加州大学并没有所谓的“主校区”，“分校”一词只是中文翻译误区）加州大学是世界最大的大学联邦体，其旗下大学在各项学术指标和排名中均名列前茅。这些校区互为独立又紧密联系，各自作为独立的大学而存在，共同组成了享誉全美乃至全世界的加州大学。', '公立', '美国', 3, 'Computer Science-Artificial Intelligence (AI)', 0, '学术型硕士', 'https://eecs.berkeley.edu/academics/graduate/research-programs/', 'College of Engineering', 'Department of Electrical Engineering & Computer Sciences，The Berkeley Artificial Intelligence Research (BAIR) Lab', NULL, 'https://eecs.berkeley.edu/academics/graduate/research-programs/admissions/', 'Master of Science in Computer Science', '1 Year to Less than 5 Years', '可选', '2年制学位-24units', 'https://eecs.berkeley.edu/book/ms/thesis-report/', 'The Master of Science (MS) program is a very small research program for exceptional applicants with research experience. Both (M.S./Ph.D.)ull program for those who know they would like to pursue a Ph.D at Berkeley.', 'Leads to career in industrial R&D or Ph.D/Research preparation for the PhD.', 0, NULL, 1, 'https://www2.eecs.berkeley.edu/Courses/CS/', 'https://www2.eecs.berkeley.edu/Research/Areas/AI/', 'https://docs.google.com/document/d/1x_H1yoRZFbRiyr76N6MMqUSxngfl9alKt3FnP2tiUhw/edit#heading=h.2vgusr5pkrih');

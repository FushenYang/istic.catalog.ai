INSERT INTO "degree_prog" ("degree_prog_id", "inst_name_cn", "inst_name_en", "intro_cn", "inst_type", "country", "rank", "prog_name", "is_indep", "prog_type", "prog_url", "school", "host_inst", "awards_or_abet", "apply_url", "degree", "duration", "joint", "min_credit", "thesis", "prog_goal", "prog_outcome", "has_culm_act", "internship ", "has_minor", "course_url", "faculty", "stu_hb_url") VALUES (23, '芝加哥大学', 'The University of Chicago', '芝加哥大学（The University of Chicago），简称“芝大”（UChicago），世界顶尖私立研究型大学，常年稳居多家世界大学排行榜前十位，坐落于美国国际金融中心芝加哥。该校诞生了芝加哥经济学派等众多知名的芝加哥学派，创立了法律经济学，是经济学、法学等社会科学学科的世界级学术重镇；而从曼哈顿计划起大批科学家汇集于此，在“原子能之父”恩利克·费米等人的领导下建立了世界上第一台核反应堆（芝加哥一号堆），开启了人类原子能时代，并创办了美国第一所国家实验室阿贡国家实验室和著名的费米实验室，奠定了芝大在自然科学界的重要地位。芝加哥大学于1890年由石油大王约翰·洛克菲勒创办，素以盛产诺贝尔奖得主而闻名，约40%的诺贝尔经济学奖得主与芝大相关。截至至2019年10月，芝加哥大学的校友、教授及研究人员中，共产生了100位诺奖得主、位列世界第四。华裔诺贝尔物理学奖得主杨振宁、李政道、崔琦均在芝加哥大学取得物理学博士学位，另有9位菲尔兹奖得主（世界第七）、4位图灵奖得主及25位普利策奖得主曾在芝大工作或学习。美国第44任总统奥巴马曾长期在芝大法学院教授宪法（1992-2004年）。2019-20年度，芝加哥大学位列U.S. News美国最佳大学排名第6、THE世界大学声誉排名第10。2020-21年度，U.S. News世界大学排名第13、软科世界大学学术排名第10、THE世界大学排名第10、QS世界大学排名第9。', '私立', '美国', 35, 'Computer Science-AI&Machine Learning', 0, '学士', 'http://collegecatalog.uchicago.edu/thecollege/computerscience/', 'The College ', 'department of computer science', NULL, 'https://collegeadmissions.uchicago.edu/apply', 'Bachelor of Science', '1 Year to Less than 5 Years', '否', '4200units', 'https://cs.uchicago.edu/academics/undergraduate/bx-thesis-program/', 'prepares students for either graduate work or employment in computer science.', 'Recipients of the BS will also have substantial depth and breadth in a field outside of computer science through the completion of an approved related area.', 0, NULL, 1, 'http://collegecatalog.uchicago.edu/thecollege/computerscience/', 'https://cs.uchicago.edu/people/uchicago-faculty/full-time-faculty/?_research_fselect=ai-machine-learning', NULL);

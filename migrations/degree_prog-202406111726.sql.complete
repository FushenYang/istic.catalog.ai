INSERT INTO "degree_prog" ("degree_prog_id", "inst_name_cn", "inst_name_en", "intro_cn", "inst_type", "country", "rank", "prog_name", "is_indep", "prog_type", "prog_url", "school", "host_inst", "awards_or_abet", "apply_url", "degree", "duration", "joint", "min_credit", "thesis", "prog_goal", "prog_outcome", "has_culm_act", "internship ", "has_minor", "course_url", "faculty", "stu_hb_url") VALUES (56, NULL, NULL, NULL, 'Government', '美国', NULL, 'Stokes Educational Program', NULL, '非学位', 'https://apply.intelligencecareers.gov/student-tiles', NULL, NULL, NULL, NULL, NULL, '6 Months to Less than 1 Year', NULL, NULL, NULL, 'The Stokes Educational Scholarship Program, formerly known as the Undergraduate Training Program, facilitates the recruitment of individuals, particularly minority high school students, who have demonstrated skills critical to NSA. Stokes is open to high school seniors planning on majoring in computer science or computer/electrical engineering.', NULL, NULL, NULL, NULL, NULL, NULL, NULL);
INSERT INTO "degree_prog" ("degree_prog_id", "inst_name_cn", "inst_name_en", "intro_cn", "inst_type", "country", "rank", "prog_name", "is_indep", "prog_type", "prog_url", "school", "host_inst", "awards_or_abet", "apply_url", "degree", "duration", "joint", "min_credit", "thesis", "prog_goal", "prog_outcome", "has_culm_act", "internship ", "has_minor", "course_url", "faculty", "stu_hb_url") VALUES (57, NULL, NULL, NULL, 'Government', '美国', NULL, 'CSGrad4U', NULL, '非学位', 'https://www.nsf.gov/pubs/2021/nsf21044/nsf21044.jsp', NULL, NULL, NULL, NULL, NULL, 'Variable Time', NULL, NULL, NULL, 'The CSGrad4US Fellowships support graduate study leading to research-based doctoral degrees in CISE disciplines. They support individuals proposing plans for graduate education that take individual experiences, interests and competencies into consideration. A CSGrad4US Fellowship applicant''s plan describes the academic and industrial achievements, attributes, and experiences that illustrate their demonstrated potential for significant research achievements. A CSGrad4US Fellowship applicant must provide a profile of their relevant education and experience and a reference letter that demonstrates this potential.', NULL, NULL, NULL, NULL, NULL, NULL, NULL);
INSERT INTO "degree_prog" ("degree_prog_id", "inst_name_cn", "inst_name_en", "intro_cn", "inst_type", "country", "rank", "prog_name", "is_indep", "prog_type", "prog_url", "school", "host_inst", "awards_or_abet", "apply_url", "degree", "duration", "joint", "min_credit", "thesis", "prog_goal", "prog_outcome", "has_culm_act", "internship ", "has_minor", "course_url", "faculty", "stu_hb_url") VALUES (58, NULL, NULL, NULL, 'Government', '美国', NULL, 'Cyber Summer Program', NULL, '非学位', 'https://apply.intelligencecareers.gov/student-tiles', NULL, NULL, NULL, NULL, NULL, '1 Week to Less than 6 Month', NULL, NULL, NULL, 'The Cyber Summer Program (CSP) is the National Security Agency''s (NSA) premier outreach effort to the very best undergraduate and graduate computer science, engineering, mathematics, network security and information assurance students in the country. Each summer we invite up to 24 exceptional students to participate in a 12-week program where they work together, and in teams, directly with NSA technical professionals on mission-critical cyber-related problems.', NULL, NULL, NULL, NULL, NULL, NULL, NULL);

INSERT INTO "degree_prog" ("degree_prog_id", "inst_name_cn", "inst_name_en", "intro_cn", "inst_type", "country", "rank", "prog_name", "is_indep", "prog_type", "prog_url", "school", "host_inst", "awards_or_abet", "apply_url", "degree", "duration", "joint", "min_credit", "thesis", "prog_goal", "prog_outcome", "has_culm_act", "internship ", "has_minor", "course_url", "faculty", "stu_hb_url") VALUES (34, '宾夕法尼亚大学', 'University of Pennsylvania', '宾夕法尼亚大学（University of Pennsylvania ），简称宾大（UPenn），位于宾夕法尼亚州最大城市费城，是一所全球顶尖的私立研究型大学，著名的八所常春藤盟校之一，北美顶尖大学学术联盟美国大学协会14所创始成员之一。宾大由本杰明·富兰克林创建于1740年，是美国第四古老的高等教育机构，也是美国第一所从事科学技术和人文教育的现代高等学校。1765年成立全美第一所医学院、1881年成立全美第一所商学院、1896成立全美第一个学生会组织，在至今数百年的学校历史中，宾大一次又一次在教育以及科学技术上推动了人类文明的发展，诞生了人类历史上第一台通用电子计算机ENIAC，被誉为现代计算机科学文明的发源地，以及风疹疫苗、乙肝疫苗、认知心理疗法等的发明挽救了无数生命。宾大拥有全世界最强大的校友网络之一，包括数十位在各自领域的最高奖项获得者（如诺贝尔奖、图灵奖等）以及许多美国国家院士。宾夕法尼亚大学名列2019福布斯美国大学排行榜第6名，2020U.S. News美国最佳大学排名第6名，2020莫斯科国际大学排名第5名，2020U.S. News世界大学排名第14名，2020THE世界大学排名第11名，被普林斯顿评论评为十大梦想学校之一。根据研究经费及师生质量等学术指标，2014年，大学评定中心把宾大评为研究实力一级研究型大学（并列一级另有哥大、哈佛、麻省理工和斯坦福四所高校）。根据2017年公布的福布斯富豪榜，宾大培养的亿万富豪校友数量位居全美第一，超过哈佛大学和斯坦福大学。', '私立', '美国', 20, 'Artificial Intelligence', 1, '学士', 'https://ai.seas.upenn.edu/', 'School of Engineering and Applied Science', 'Departments of Electrical and Systems Engineering (ESE) and Computer and Information Science (CIS)', 'The First Ivy League Bachelor of Science in Engineering in Artificial Intelligence', 'https://admissions.upenn.edu/how-to-apply/apply-for-admissions', 'B.S.E. in Artificial Intelligence', '1 Year to Less than 5 Years', '否', '37units', NULL, 'Complementing these engineering skills with a broader perspective, students learn about intelligence from a cognitive science perspective, and they develop a sense of the issues (and solutions) required to responsibly develop AI to benefit society. ', ' Finally, students choose a concentration, ranging from machine learning, to vision and language, to data and society, to robotics, to AI and health systems.', 0, NULL, 1, 'https://ai.seas.upenn.edu/curriculum/', 'https://directory.seas.upenn.edu/computer-and-information-science/', 'https://ugrad.seas.upenn.edu/student-handbook/');

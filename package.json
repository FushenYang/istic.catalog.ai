{
  "private": true,
  "name": "remix-cloudflare-template",
  "type": "module",
  "description": "All-in-one remix starter template for Cloudflare Pages",
  "scripts": {
    "dev": "remix vite:dev",
    "test": "playwright test --ui",
    "migrate": "wrangler d1 migrations apply DB --local",
    "start": "wrangler pages dev ./build/client --d1=DB",
    "build": "remix vite:build",
    "cleanup": "rimraf .cache ./build",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "wrangler types && tsc",
    "prepare": "husky"
  },
  "dependencies": {
    "@markdoc/markdoc": "^0.4.0",
    "@mdi/js": "^7.4.47",
    "@mdi/react": "^1.6.1",
    "@prisma/adapter-d1": "^5.12.1",
    "@prisma/client": "^5.12.1",
    "@remix-run/cloudflare": "^2.8.1",
    "@remix-run/cloudflare-pages": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "isbot": "^3.8.0",
    "papaparse": "^5.4.1",
    "prisma": "^5.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240405.0",
    "@octokit/types": "^12.6.0",
    "@playwright/test": "^1.43.1",
    "@remix-run/dev": "^2.8.1",
    "@remix-run/eslint-config": "^2.8.1",
    "@tailwindcss/typography": "^0.5.12",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.13",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.9",
    "vite-tsconfig-paths": "^4.3.2",
    "wrangler": "^3.50.0"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "lint-staged": {
    "*.{js,mjs,ts,tsx,css,md,yml}": "prettier --write"
  }
}
